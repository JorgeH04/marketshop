                    <div class="">
                        <h2 class="title-div wow slideInRight" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Our Latest Products available</h2>
                        <div class="products">
                            <div class="row">

                            <% produno.forEach(function (produn) { %>
                                <div class="col-md-3">
                                    <div class="product-item">
                                        <div class="product-borde-inner">
                                            <a href="/produnoredirect/<%= produn.id %>">
                                                <img src="<%= produn.image %>" class="img img-responsive"/>
                                            </a> 

                                            <div class="product-price">
                                                <a href="product_single.html">DSLR Camera</a><br />
                                                <span class="prev-price">
                                                    <del></del>
                                                </span>
                                                <span class="current-price">
                                                    $<%= produn.price %>
                                                </span>
                                            </div>

                                            <a href="/addtocardproduno/<%= produn.id %>"  class="btn btn-cart text-center add-to-cart pull-right">
                                                <i class="fa fa-cart-plus"></i>
                                                Add to cart
                                            </a>
                                            <div class="clear"></div>
                                        </div>
                                    </div> 
								</div><!-- End Latest products[single] -->  
                                <% }) %>    
                                
                                
                                
                  

                                <div class="clear"></div>



                                <div class="col-12">
                                        <div class="row">
                                              <!-- pagination -->
                                              <% if(pages > 0) { %> <!-- show pagination if there is pages -->
                                                <nav  class="mx-auto" style="margin-bottom: 50px">
                                                  <ul class="pagination">
                                                    <!-- FIRST ITEM -->
                                                    <% if(current == 1) { %>
                                                      <li class="page-item disabled">
                                                        <a class="page-link" href="#">First</a>
                                                      </li>
                                                    <% } else { %>
                                                      <li class="page-item">
                                                        <a class="page-link" href="/produnoindex/1">First</a>
                                                      </li>
                                                    <% } %>
                                                    <!-- ITEMS  -->
                                                    <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                                    <% if(i !== 1) { %>
                                                      <li class="page-item disabled">
                                                        <a class="page-link" href="#">...</a>
                                                      </li>
                                                    <% } %>
                                                    <% for(; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                                      <% if(i == current) { %>
                                                        <li class="page-item active">
                                                          <a class="page-link" href="<%= i %>">
                                                            <%= i %>
                                                          </a>
                                                        </li>
                                                      <% } else { %>
                                                        <li class="page-item">
                                                          <a class="page-link" href="/produnoindex/<%= i %>">
                                                            <%= i %>
                                                          </a>
                                                        </li>
                                                      <% } %>
                                                      <% if (i == Number(current) + 4 && i < pages) { %>
                                                         <li class="page-item disabled">
                                                           <a class="page-link" href="#">...</a>
                                                         </li>
                                                      <% } %>
                                                    <% } %>
                                                    <!-- LAST ITEM -->
                                                    <% if(current == pages) { %>
                                                      <li class="page-item disabled">
                                                        <a class="page-link" href="#">
                                                          Last
                                                        </a>
                                                      </li>
                                                    <% } else { %>
                                                      <li class="page-item">
                                                        <a class="page-link" href="/produnoindex/<%= pages %>">
                                                          Last
                                                        </a>
                                                      </li>
                                                    <% } %>
                                                  </ul>
                                                </nav>
                                              <% } %>
                                            </div>
