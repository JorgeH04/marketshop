      <h2 class="title-div wow slideInLeft pull-left" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Productos destacados</h2><div class="clear"></div>
      <div class="featured-navigation pull-right">
          <span class="">
              <a class="owl-prev owl-navigaiton"><i class="fa fa-angle-double-right"></i></a>
          </span>
          <span class="stop">
              <a class="owl-next owl-navigaiton">||</a>
          </span>
          <span class="">
              <a class="owl-next owl-navigaiton"><i class="fa fa-angle-double-left"></i></a>
          </span>
          
      </div>
      <div class="clear"></div>
      <div class="featured-items">
          <!-- Set up your HTML -->
          <div class="owl-carousel">
              <% ofertauno.forEach(function (ofertaun) { %>
              <div class="item featured2">
                  <div class="item-full animated featured2-inner  width0">
                    <a href="/ofertaunoredirect/<%= ofertaun.id %>"><h4><%= ofertaun.title %></h4></a>
