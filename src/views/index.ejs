                      <a href="/addtocardofertauno/<%= ofertaun.id %>" class="btn btn-cart">
                          Add To Cart
                      </a>                  
                  </div>
                  
                      <img src="<%= ofertaun.image %>" class="img img-responsive" alt="Product1">
                  
              </div> <!-- Single Featured Item -->      
              <% }) %>
          
                            
              
                          
              
              


              
              
          </div>
      </div>
  </div>
</div> <!--End Featured products Div-->


